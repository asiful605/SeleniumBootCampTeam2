<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="at_t Automation Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="10"/>
    <!--        <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="83"/>
    <parameter name="url" value="https://www.cigna.com/"/>

    <test name="CignaHomePage">
        <classes>
            <class name="hometest.CignaHomePageTest"></class>
            <class name="hometest.CignaInternationalTest"></class>

        </classes>

    </test>

